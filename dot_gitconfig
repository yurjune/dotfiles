[merge]
	ff = false
[push]
	autoSetupRemote = true
[core]
	excludesfile = /Users/jerry/.gitignore_global
[commit]
	template = /Users/jerry/.stCommitMsg
[alias]
	lg = log -1000
	lo = log --oneline -1000
	lgg = log --oneline --graph --decorate -1000
	cm = commit -m
	cp = cherry-pick
	rhh = reset --hard HEAD^
	rsh = reset HEAD^
	ri = rebase -i
	rc = rebase --continue
	ra = rebase --abort
	aa = add --all
	sc = switch -c
	st = stash push -u
	stl = stash list
	stp = stash pop
	sta = stash apply
[rerere]
	enabled = false
[color "diff"]
	# catpuccin mocha green
	new = "#a6e3a1"
	# catpuccin mocha red
	old = "#f38ba8"
